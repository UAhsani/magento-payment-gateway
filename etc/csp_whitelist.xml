<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <!--https://devdocs.magento.com/guides/v2.4/extension-dev-guide/security/content-security-policies.html-->
    <policies>
        <policy id="script-src">
            <values>
                <value id="geidea-scripts" type="host">*.geidea.net</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="geidea-bodies" type="host">*.geidea.net</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="geidea-images" type="host">*.geidea.net</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="geidea-fonts" type="host">*.geidea.net</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="geidea-styles" type="host">*.geidea.net</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
